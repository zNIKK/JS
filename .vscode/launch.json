    "version": "0.2.0",
    "configurations": [
    {
        "name": "Launch Program",
        "program": "${fileDirname}/${fileBasename}",
        "request": "launch",
        "skipFiles": [
            "<node_internals>/**"
        ],
        "type": "node"
    },
        {
            "version": "0.1.0",
            "configurations": [
                {
                    "type": "devtools-for-chrome",
                    "request": "launch",
                    "name": "Launch Chrome DevTools",
                    "file": "${workspaceFolder}/index.html"
                },
                {
                    "type": "devtools-for-chrome",
                    "request": "attach",
                    "name": "Attach Chrome DevTools",
                    "url": "http://localhost:8000/"
                }
            ]
        }
        
    
    
    ]
}